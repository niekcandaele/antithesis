{
  "name": "antithesis",
  "version": "1.0.0",
  "type": "module",
  "description": "Modern Node.js + TypeScript project",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "tsx --test 'src/**/*.test.ts'",
    "test:watch": "tsx --test --watch 'src/**/*.test.ts'",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "migrate": "tsx src/lib/db/migrate-cli.ts",
    "db:codegen": "kysely-codegen --out-file src/lib/db/types.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.4",
    "@types/pg": "^8.15.5",
    "@types/triple-beam": "^1.3.5",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "kysely": "^0.28.7",
    "kysely-codegen": "^0.19.0",
    "logform": "^2.7.0",
    "pg": "^8.16.3",
    "rapidoc": "^9.3.8",
    "redis": "^5.8.2",
    "winston": "^3.18.3",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^22.9.0",
    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.14.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
